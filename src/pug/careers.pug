                                    a(href='#').block-product-menu_title О компании
                                .block-product-menu
                                    a(href='#').block-product-menu_title Новости
                                .block-product-menu
                                    a(href='#').block-product-menu_title.active-block Вакансии
                            .wrapper-card-product_left-careers
                                .block-left-careers_title Отдел кадров
                                .block-left-careers_avatar
                                    img(src='img/img-avatar.jpg' alt='')
                                .block-left-careers_name Ёлкина Екатерина
                                .block-left-careers_dev Начальник отдела кадров
                                a(href='tel:8 (800) 100-24-42').block-left-careers_tel 8 (800) 100-24-42
                                a(href='mailto:hr@omnicomm.ru').block-left-careers_email
                                    span hr@omnicomm.ru

                    .wrapper-contant-right
